<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/pool_sea" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

      

    </LinearLayout>
    
     <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewSandyBeach"
            android:text="Sandy Beach"           
            android:padding="10dp"
            android:textColor="#E8E8E8"
            android:textStyle="bold"
            android:textSize="18sp"  />

        <RatingBar
            android:id="@+id/ratingBar1"
      		android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5"/>

    </TableRow>
     <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewSports"
            android:text="Sports"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:padding="10dp"
            android:textColor="#E8E8E8"
            android:textStyle="bold"
            android:textSize="18sp"  />
 
        <RatingBar
        	android:id="@+id/ratingBar2"
      		android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5" />
    </TableRow>
     <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewWaterSports"
            android:text="Water Sports"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:padding="10dp"
            android:textStyle="bold"
            android:textColor="#E8E8E8"
            android:textSize="18sp"  />
 
        <RatingBar
       		android:id="@+id/ratingBar3"
       		android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5" />
    </TableRow>
     <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewShopping"
            android:text="Shopping"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:padding="10dp"
            android:textStyle="bold"
            android:textColor="#E8E8E8"
            android:textSize="18sp"  />
 
        <RatingBar
       		android:id="@+id/ratingBar4"
       		android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5"  />
    </TableRow>
     <TableRow
        android:id="@+id/tableRow5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewPlayground"
            android:text="Children \nPlayground"
            android:padding="10dp"
            android:textStyle="bold"
            android:textColor="#E8E8E8"
            android:textSize="18sp"  />
 
 
        <RatingBar
        	android:id="@+id/ratingBar5"
    		android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5" />
    </TableRow>
     <TableRow
        android:id="@+id/tableRow6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewGym"
            android:text="Gym"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:padding="10dp"
            android:textStyle="bold"
            android:textColor="#E8E8E8"
            android:textSize="18sp"  />
 
        <RatingBar
       		android:id="@+id/ratingBar6"
       		android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5"  />
    </TableRow>
     <LinearLayout
        android:id="@+id/tableRow7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <Button
        android:id="@+id/buttonSubmit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        
      
        android:background="#91A3B0"
        android:text="Submit"
        android:textColor="#FFFFFF"
        android:textColorLink="#91A3B0" />
    </LinearLayout>
    
    </TableLayout>