<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bar" >
     
    
    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewPoolSnackBar"
            android:text="Pool Snack \nBar "           
            android:padding="10dp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:textSize="16sp"  />
 
        <RatingBar
        android:id="@+id/ratingBarB1"
        android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5"  />
    </TableRow>
     <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <TextView
            android:id="@+id/textViewLoungeBar"
            android:text="Lounge \nBar"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:padding="10dp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:textSize="16sp"  />
 
        <RatingBar
        android:id="@+id/ratingBarB2"
        android:layout_width="wrap_content" 
      		android:layout_height="wrap_content"
       		android:numStars="3"
        	android:stepSize="0.5"
         	android:rating="0.5"  />
    </TableRow>
  
    
     <LinearLayout
        
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dip" >
 
        <Button
        android:id="@+id/buttonBSubmit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        
      
        android:background="#91A3B0"
        android:text="Submit"
        android:textColor="#FFFFFF"
        android:textColorLink="#91A3B0" />
    </LinearLayout>
    
    </TableLayout>